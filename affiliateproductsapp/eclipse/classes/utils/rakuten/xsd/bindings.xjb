<jxb:bindings version="1.0" xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<jxb:bindings schemaLocation="RakutenSchema.xsd" node="/xsd:schema">
		<jxb:bindings node="//xsd:element[@name='price']">
			<jxb:bindings node="//xsd:attribute[@name='currency']">
				<jxb:property name="currency2" />
			</jxb:bindings>
		</jxb:bindings>
	</jxb:bindings>
</jxb:bindings>